<!DOCTYPE html>
<html>
  <head>
    <title>Spotify | <%= @page_title || "Admin" %></title>
    <%= javascript_include_tag  "application" %>
    <%= stylesheet_link_tag    "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
  <div class="container">

      <header class="row">
         
      </header>
       
      <%= render(:partial => "application/menu") %>  
      

      <div class="row">
          <% if !flash[:notice].blank? %>
            <div class="alert alert-success" role="alert">
              <%= flash[:notice] %>
            </div>
          <% end %>    
      </div>

      <%= yield %>

      <nav class="navbar navbar-default navbar-fixed-bottom" role="navigation">
          <div class="container">
            </br>
            <p class="text-center"> Gabriel Poblete / https://github.com/gpcweb </p> 
          </div>
      </nav>
  </div>    

  </body>
</html>
